{"controller":"[\"$scope\", function($scope){\n    $scope.filter=function(){\n        $scope.filter.ing=!$scope.filter.ing;\n    };\n    $scope.orders=[];\n    for (var cx=0,dx=Math.random()*100;cx<dx;cx++){\n        $scope.orders.push({\n            hpxx:$scope.$eval(\"'deploy'|random\"),\n            hp:$scope.$eval(\"'format'|random\"),\n            status:parseInt(Math.random()*10),\n            junj:$scope.$eval(\"1000|random:2\"),\n            jinj:$scope.$eval(\"200|random:2\"),\n            xj:Math.random()>.3,\n            dxj:$scope.$eval(\"2000|random:2\"),\n            cb:$scope.$eval(\"1200|random:2\"),\n            man:$scope.$eval(\"'name'|random\"),\n            time:$scope.$eval(\"'date'|random\")\n        });\n    }\n}]","template":"<style>.setting-info-retail_man .container {\n  border: 1px solid #e3e6eb;\n  position: absolute;\n  left: 12px;\n  top: 12px;\n  right: 12px;\n  bottom: 12px;\n}\n.setting-info-retail_man .container .top-bar {\n  height: auto;\n  overflow: auto;\n  line-height: 53px;\n  padding: 0 10px;\n  text-align: right;\n}\n.setting-info-retail_man .container .top-bar .field {\n  width: 352px;\n  float: left;\n  text-align: justify;\n}\n.setting-info-retail_man .container .top-bar .anniu {\n  margin: 0 9px;\n}\n.setting-info-retail_man .container .top-bar .anniu.r9 {\n  margin: 0 36px;\n}\n.setting-info-retail_man .container .grid {\n  position: absolute;\n  width: 100%;\n  height: auto;\n  left: 0;\n  top: 53px;\n  right: 0;\n  bottom: 0;\n  /*transition: top 0.1s ease-out;*/\n}\n</style><div class=\"container\" row-view>\n    <div class=\"top-bar\">\n\n        <field name=\u5546\u54c1\u7c7b\u578b><selector></selector></field>\n        <field name=\u8d27\u54c1\u72b6\u6001><selector></selector></field>\n        <field name=\u8d27\u54c1\u72b6\u6001><selector></selector></field>\n        <field name='\u6709\u6548\u6807\u5fd7'> \n            <checkbox>\u5df2\u542f\u7528</checkbox>\n            <checkbox>\u672a\u542f\u7528</checkbox>\n            <checkbox>\u4e0d\u663e\u793a0</checkbox>\n        </field>\n        <anniu class='r9'>\u67e5\u8be2</anniu>\n        <anniu>\u5bfc\u51fa</anniu>\n        <anniu ng-click='filter()'>{{filter.ing?\"\u53d6\u6d88\u8fc7\u6ee4\":'\u8fc7\u6ee4'}}</anniu>\n    </div>\n    <grid ng-model='orders' show-filter=\"filter.ing\">\n        <div name=\u8d27\u54c1\u4fe1\u606f field=\"hpxx\" ></div>\n        <div name=\u8d27\u54c1 field=\"hp\" ></div>\n        <div name=\u8d27\u54c1\u72b6\u6001 field=\"status\" ></div>\n        <div name=\u5e93\u5b58\u5747\u4ef7 field=\"junj\" ></div>\n        <div name=\u6700\u8fd1\u8fdb\u4ef7 field=\"jinj\" ></div>\n        <div name=\u542f\u7528\u9650\u4ef7 field=\"xj\" ></div>\n        <div name=\u6700\u4f4e\u9650\u4ef7 field=\"dxj\" ></div>\n        <div name=\u8003\u6838\u6210\u672c field=\"cb\" ></div>\n        <div name=\u4fee\u6539\u4eba field=\"man\" ></div>\n        <div name=\u4fee\u6539\u65e5\u671f field=\"time\" ></div>\n        <div name=\u64cd\u4f5c template=\"edit\" nofilter fixwidth width=\"132px\"></div>\n    </grid>\n</div>\n"}