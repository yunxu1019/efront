{"controller":" [\"$scope\", \"user\", \"$compile\", \"isie\", function ($scope, user, $compile,isie) {\n    $scope._user = user();\n    $scope.menus = [\n        [\"\u4e2a\u4eba\u8bbe\u7f6e\", \"icon-person-setting\", function () {}],\n//        [\"\u4fee\u6539\u5bc6\u7801\", \"fa-lock\", function () {}],\n        [\"\u77e5\\u2002\u8bc6\\u2002\u5e93\", \"icon-iconfontzhishiku1\", function () {}],\n        [\"\u5e2e\\u2003\\u2003\u52a9\", \"icon-bangzhu\", function () {}],\n\n        [\"\u9000\\u2003\\u2003\u51fa\", \"icon-tuichu\", function () {\n                user.logout();\n            }]\n    ];\n    var context = angular.element(\"<div class='context'>\\\n<div ng-repeat='m in menus' class=item ng-mousedown='m[2]()' >\\\n<i class='iconfont' ng-class=m[1]></i>\\\n<span ng-bind=m[0]></span>\\\n</div><div class='triangle'></div></div>\");\n    $compile(context)($scope);\n    $scope.show = function (event) {\n        if (isie()) {\n            var input = event.target;\n            //\u4ec5ie\u4f7f\u7528\u53bb\u9664\u5149\u6807\n            input.type = \"button\";\n            input.focus();\n            //\u82f9\u679c\u4e0a\u8fd9\u4e48\u7528\u4f1a\u5931\u6548\n        }\n        document.body.appendChild(context[0]);\n    };\n    $scope.hide = function () {\n        context[0].parentNode && context[0].parentNode.removeChild(context[0]);\n    };\n    $scope.$on(\"$destroy\", $scope.hide);\n}]","template":"<style>.common-nav {\n  height: 50px;\n  border-bottom: 1px solid #ccc;\n  background-color: #363d4d;\n  background-image: url(\"data:image/png;base64,\");\n  background-repeat: no-repeat;\n  background-size: 34px 35px;\n  background-position: 62px center;\n  padding-left: 126px;\n  text-align: right;\n  color: #fff;\n}\n.common-nav .Scroller-Container {\n  overflow: hidden;\n  height: 100%;\n  width: 100%;\n}\n.common-nav .scroll {\n  display: none;\n}\n.common-nav .title {\n  margin-left: 116px;\n  width: auto;\n  top: 0;\n  bottom: 0;\n  position: relative;\n